{
  "\\s": {
    "prefix": "\\s",
    "body": ["\\section{$1}", "$0"],
    "description": "Start a section"
  },
  "\\ss": {
    "prefix": "\\ss",
    "body": ["\\subsection{$1}", "$0"],
    "description": "Start a subsection"
  },
  "\\sss": {
    "prefix": "\\sss",
    "body": ["\\subsubsection{$1}", "$0"],
    "description": "Start a subsubsection"
  },
  "\\i": {
    "prefix": "\\i",
    "body": ["\\begin{itemize}", "  \\item $0", "\\end{itemize}"],
    "description": "Start an unordered list"
  },
  "\\e": {
    "prefix": "\\e",
    "body": ["\\begin{enumerate}", "  \\item $0", "\\end{enumerate}"],
    "description": "Start an ordered list"
  }
}
