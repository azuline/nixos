{
	"s": {
		"prefix": ["s"],
		"body": [
			"\\section{$1}",
			"$0"
		]
	},
	"ss": {
		"prefix": ["ss"],
		"body": [
			"\\subsection{$1}",
			"$0"
		]
	},
	"sss": {
		"prefix": ["sss"],
		"body": [
			"\\subsubsection{$1}",
			"$0"
		]
	},
	"s*": {
		"prefix": ["s*"],
		"body": [
			"\\section*{$1}",
			"$0"
		]
	},
	"ss*": {
		"prefix": ["ss*"],
		"body": [
			"\\subsection*{$1}",
			"$0"
		]
	},
	"sss*": {
		"prefix": ["sss*"],
		"body": [
			"\\subsubsection*{$1}",
			"$0"
		]
	},
	"it": {
		"prefix": ["it"],
		"body": [
			"\\begin{itemize}[noitemsep]",
			"  \\item $0",
			"\\end{itemize}"
		]
	},
	"en": {
		"prefix": ["en"],
		"body": [
			"\\begin{enumerate}[noitemsep]",
			"  \\item $0",
			"\\end{enumerate}"
		]
	},
	"-": {
		"prefix": ["-"],
		"body": [
			"\\item $0"
		]
	},
	"i": {
		"prefix": ["i"],
		"body": [
			"\\ti{$1}$0"
		]
	},
	"b": {
		"prefix": ["b"],
		"body": [
			"\\tb{$1}$0"
		]
	},
	"t": {
		"prefix": ["t"],
		"body": [
			"\\hypertarget{$1}{",
			"}"
		]
	},
	"l": {
		"prefix": ["l"],
		"body": [
			"\\hl{$1}{$2}$0"
		]
	},
	"li": {
		"prefix": ["li"],
		"body": [
			"\\hl{$1}{\\ti{$2}}$0"
		]
	},
	"f": {
		"prefix": ["f"],
		"body": [
			"\\fn{$1}$0"
		]
	},
	"fc": {
		"prefix": ["fc"],
		"body": [
			"\\fn{\\fc{$1}}$0"
		]
	},
	"fcc": {
		"prefix": ["fcc"],
		"body": [
			"\\fn{\\fc[$2]{$1}}$0"
		]
	},
	"q": {
		"prefix": ["q"],
		"body": [
			"\\begin{quote}",
      "  $0",
      "\\end{quote}"
		]
	}
}
